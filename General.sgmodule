简书重定向1 = type=http-request,pattern=^https:\/\/links\.jianshu\.com\/go\?to=,script-path=Scripts/noredirect.js
简书重定向2 = type=http-request,pattern=^https:\/\/www\.jianshu\.com\/go-wild\?ac=\d&url=,script-path=Scripts/noredirect.js
# 微博
微博重定向1 = type=http-request,pattern=^https?:\/\/weibo\.cn\/sinaurl\?(.*&)?(u|toasturl|goto)=,script-path=Scripts/noredirect.js
微博重定向2 = type=http-request,pattern=^https:\/\/shop\.sc\.weibo\.com\/h5\/jump\/error\?(.*&)?url=(.*),script-path=Scripts/noredirect.js
微博重定向3 = type=http-response,pattern=^https?:\/\/(sinaurl|t)\.cn\/,script-path=Scripts/noredirect.js
