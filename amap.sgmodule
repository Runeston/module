#!name=❏ 高德地图
#!desc=广告拦截
#!from=RuCu6、Key

[Rule]
DOMAIN-SUFFIX,amdc.m.taobao.com,REJECT
URL-REGEX,\/amdc\/mobileDispatch,REJECT

[Script]
# 首页卡片
amap = type=http-response,pattern=^https:\/\/m5\.amap\.com\/ws\/faas\/amap-navigation\/main-page\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1
# 我的卡片
amap = type=http-response,pattern=^https:\/\/m5\.amap\.com\/ws\/shield\/dsp\/profile\/index\/nodefaasv3\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1
# 首页右上角动图
amap = type=http-response,pattern=^https:\/\/m5\.amap\.com\/ws\/shield\/frogserver\/aocs\/updatable\/1\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1
# 附近页面
amap = type=http-response,pattern=^https:\/\/m5\.amap\.com\/ws\/shield\/search\/nearbyrec_smart\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1
# 地点详情页
amap = type=http-response,pattern=^https:\/\/m5\.amap\.com\/ws\/shield\/search\/poi\/detail\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1
# 开屏广告
amap = type=http-response,pattern=^https:\/\/m5\.amap\.com\/ws\/valueadded\/alimama\/splash_screen\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1
# 打车卡片
amap = type=http-response,pattern=^https:\/\/m5-zb\.amap\.com\/ws\/promotion-web\/resource\?,requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/Runeston/script/main/amap.js,script-update-interval=-1

[Map Local]
# 路线规划
^https:\/\/m5\.amap\.com\/ws\/faas\/amap-navigation\/card-service-route-plan\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
# 首页消息横幅
^https:\/\/m5\.amap\.com\/ws\/faas\/amap-navigation\/main-page-assets\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
^https:\/\/sns\.amap\.com\/ws\/msgbox\/pull\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
^https:\/\/sns\.amap\.com\/ws\/msgbox\/pull_mp\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
# 搜索框热词
^https:\/\/m5\.amap\.com\/ws\/shield\/search\/new_hotword\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
# 其他
^https:\/\/m5\.amap\.com\/ws\/asa\/ads_attribution\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
^https:\/\/m5\.amap\.com\/ws\/shield\/scene\/recommend\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
# 首页天气
^https:\/\/m5\.amap\.com\/ws\/valueadded\/weather\/v2\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"
# 打车弹窗
^https:\/\/m5-zb\.amap\.com\/ws\/boss\/order_web\/friendly_information\? data="https://raw.githubusercontent.com/Runeston/script/main/reject-dict.json"

[MITM]
hostname = %APPEND% m5.amap.com, m5-zb.amap.com, sns.amap.com